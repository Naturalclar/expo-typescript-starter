{
  "name": "expo-typescript-starter",
  "version": "0.1.0",
  "description": "Template for expo project with TS, Storybook, Reg-Suit",
  "repository": "https://github.com/Naturalclar/expo-typescript-starter.git",
  "license": "MIT",
  "author": "Jesse Katsumata <jesse.katsumata@gmail.com>",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "android": "expo start --android",
    "build": "yarn storybook:build -o public",
    "eject": "expo eject",
    "ios": "expo start --ios",
    "lint": "eslint . --ext '.ts, .tsx'",
    "new": "scaffdog generate",
    "reg-suit": "reg-suit run",
    "start": "expo start",
    "storybook": "start-storybook -p 9001 -c .storybook -s .storybook/assets",
    "storybook:build": "build-storybook -c .storybook -s .storybook/assets",
    "storycap:all": "rm -rf ./__screenshots__ && yarn storycap --serverCmd 'yarn storybook --ci' --viewport '414x736' --serverTimeout '200000'",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "type-check": "tsc --noEmit"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^2.1.0",
    "@expo/samples": "^3.0.3",
    "@expo/vector-icons": "^10.0.0",
    "expo": "^37.0.0",
    "expo-asset": "~8.1.3",
    "expo-font": "~8.1.0",
    "react": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.0.tar.gz",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-paper": "^3.6.0",
    "react-native-reanimated": "~1.7.0",
    "react-navigation": "^3.11.0",
    "react-redux": "7.1.3",
    "redux": "^4.0.4",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.9.0",
    "@octokit/rest": "^16.16.0",
    "@react-native-community/eslint-config": "0.0.7",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-options": "^5.2.5",
    "@storybook/addon-viewport": "^5.2.5",
    "@storybook/react": "^5.3.17",
    "@types/expo": "^32.0.13",
    "@types/expo__vector-icons": "9.0.1",
    "@types/jest": "^24.0.0",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.62.1",
    "@types/react-redux": "^7.1.7",
    "@types/react-test-renderer": "^16.9.2",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-persist": "^4.3.1",
    "@types/redux-thunk": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.4.2",
    "babel-loader": "^8.1.0",
    "babel-preset-expo": "^8.1.0",
    "cache-loader": "^4.1.0",
    "eslint": "^5.13.0",
    "eslint-plugin-prettier": "^3.1.2",
    "fork-ts-checker-webpack-plugin": "^1.0.0",
    "jest-expo": "^37.0.0",
    "metro-react-native-babel-preset": "0.58.0",
    "node-fetch": "^2.3.0",
    "now": "^15.8.7",
    "prettier": "^2.0.2",
    "react-art": "16.9.0",
    "react-dom": "16.9.0",
    "react-native-elements": "^1.2.7",
    "react-native-vector-icons": "^6.5.0",
    "react-native-web": "^0.11.7",
